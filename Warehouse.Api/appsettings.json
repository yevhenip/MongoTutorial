{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Data": {
    "ConnectionString": "mongodb://localhost:27017"
  },
  "JwtTokenConfiguration": {
    "Secret": "SuperSecretSuperSecretSuperSecretSuperSecretSuperSecretSuperSecretSuperSecretSuperSecret",
    "Issuer": "https://mongoTutorial.com",
    "Audience": "https://mongoTutorial.com",
    "AccessTokenExpirationMinutes": 10080,
    "RefreshTokenExpirationMinutes": 10080
  },
  "Cache": {
    "Configuration": "127.0.0.1",
    "InstanceName": "warehouse",
    "CacheOptions": {
      "Product": {
        "AbsoluteExpiration": 10,
        "SlidingExpiration": 2
      },
      "Manufacturer": {
        "AbsoluteExpiration": 10,
        "SlidingExpiration": 2
      },
      "User": {
        "AbsoluteExpiration": 10,
        "SlidingExpiration": 2
      },
      "Customer": {
        "AbsoluteExpiration": 10,
        "SlidingExpiration": 2
      }
    }
  },
  "RabbitMq": {
    "ConnectionString": "host=localhost"
  },
  "Polly": {
    "RepeatedTimes": 3,
    "RepeatedDelay": 2
  },
  "FileSettings": {
    "MaxBytes": 10485760,
    "AcceptedFileTypes": [
      ".csv",
      ".json"
    ]
  }
}
